<%= form_tag session_path do %>

  <%= label_tag do %>
    Username:
    <%= text_field_tag :username %>
  <% end %>

  <%= label_tag do %>
    Password:
    <%= password_field_tag :password %>
  <% end %>

  <%= submit_tag "Login!" %>
<% end %>

<%= link_to "Sign up for Generic Site!", new_user_path %>